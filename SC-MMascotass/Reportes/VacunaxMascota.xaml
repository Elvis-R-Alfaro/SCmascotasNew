             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SC_MMascotass.Reportes"
             mc:Ignorable="d" 
             d:DesignHeight="480" d:DesignWidth="520">
